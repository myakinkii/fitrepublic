{
	"_version": "1.5.0",
	"sap.app": {
		"id": "ru.fitrepublic.ui5_clientApp",
		"type": "application",
		"i18n": "shared/i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/odata/client",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ru.fitrepublic.ui5_clientApp.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.integration": {},
				"sap.m": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ru.fitrepublic.shared.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "shared/css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ru.fitrepublic.ui5_clientApp.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
			},
			"routes": [{
				"name": "onboard",
				"pattern": "onboard/{id}",
				"target": ["onboard"]
			},{
				"name": "main",
				"pattern": "",
				"target": ["main"]
			},{
				"name": "search",
				"pattern": "search/:q:",
				"target": ["search"]
			},{
				"name": "gym",
				"pattern": "gym/{id}",
				"target": ["gym"]
			},{
				"name": "coach",
				"pattern": "coach/{id}",
				"target": ["coach"]
			},{
				"name": "clients",
				"pattern": "clients",
				"target": ["clients"]
			},{
				"name": "client",
				"pattern": "client/{id}",
				"target": ["client"]
			},{
				"name": "purchases",
				"pattern": "purchases",
				"target": ["purchases"]
			},{
				"name": "promos",
				"pattern": "promos",
				"target": ["promos"]
			},{
				"name": "money",
				"pattern": "money",
				"target": ["money"]
			},{
				"name": "templates",
				"pattern": "templates",
				"target": ["templates"]
			},{
				"name": "template",
				"pattern": "template/{id}",
				"target": ["template"]
			},{
				"name": "purchase",
				"pattern": "purchase/{id}/:section:",
				"target": ["purchase"]
			},{
				"name": "workouts",
				"pattern": "workouts",
				"target": ["workouts"]
			},{
				"name": "workout",
				"pattern": "workout/{id}",
				"target": ["workout"]
			},{
				"name": "exercise",
				"pattern": "workout/{id}/ex/{num}",
				"target": ["exercise"]
			},{
				"name": "schedule",
				"pattern": "schedule/:date:",
				"target": ["schedule"]
			},{
				"name": "profile",
				"pattern": "profile",
				"target": ["profile"]
			},{
				"name": "chat",
				"pattern": "chat/:id:",
				"target": ["chat"]
			}],
			"targets":{
				"onboard": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Onboard"
				},
				"main": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Main"
				},
				"search": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Search"
				},
				"gym": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Gym"
				},
				"coach": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Coach"
				},
				"clients": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "MyClients"
				},
				"client": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Client"
				},
				"purchases": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "MyPurchases"
				},
				"promos": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "MyPromos"
				},
				"money": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "MyMoney"
				},
				"templates": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "MyTemplates"
				},
				"template": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Template"
				},
				"purchase": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Purchase"
				},
				"workouts": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Workouts"
				},
				"workout": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Workout"
				},
				"exercise": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Exercise"
				},				
				"schedule": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Schedule"
				},
				"profile": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Profile"
				},
				"chat": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Chat"
				}
			}
		}
	}
}