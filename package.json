  "dependencies": {
    "@sap/cds": "^5",
    "@sap/cds-odata-v2-adapter-proxy": "^1.4.33",
    "@sap/hana-client": "^2.5.86",
    "express": "^4.17.1"
  },
  "engines": {
    "node": "^10 || ^12"
  },
  "scripts": {
    "build": "cds build",
    "_deploy": "cds deploy",
    "start": "cds run",
    "memory": "cds run --in-memory",
    "local": " CDS_ENV=local cds run",
    "deploy": "cds deploy --to sqlite:my.db --no-save"
  },
  "cds": {
    "[local]": {
      "requires": {
        "db": {
          "kind": "sqlite",
          "credentials": {
            "database": "my.db"
          }
        }
      }
    },
    "requires": {
      "db": {
        "kind": "hana",
        "model": [
          "db",
          "srv"
        ]
      }
    },
    "odata": {
      "version": "v4"
    }
  },
  "files": [
    "db",
    "srv"
  ],
  "devDependencies": {
    "sqlite3": "^5.0.0"
  }
}